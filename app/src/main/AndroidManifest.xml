<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kessseller">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.INSTALL_SHORTCUT"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_filter"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/harry_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Java.ScreenKess" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Java.AppointmentScreen" />
        <activity android:name=".Test.Test"  />
        <activity android:name=".Java.BookingScreen" />
        <activity android:name=".Java.CreateEventScreen"
            android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:name=".Java.CreateRoomScreen"
            android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:name=".Java.CreateTableScreen"
            android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:name=".Java.AddDoctorScreen" />
        <activity android:name=".Java.EventDetailStatus" />
    </application>

</manifest>